#define floatt double

*c_q   q

floatt  Kca = 3.0443904       "specify Kc"
floatt  Kci = 0.2216546       "specify Kc"
floatt  a1a =  -0.653
floatt  a2a =  0.325
floatt  a3a = 0.715
floatt  a4a = 0
floatt  a1i =  -0.653
floatt  a2i =  0.325
floatt  a3i = 0.715
floatt  a4i = 0
floatt  p1a = -.0226
floatt  p1i = -.0226
floatt  p2a = 0
floatt  p2i = 0
bool    fita1 = +
bool    fita2 = +
bool    fita3 = +
bool    fita4 = -
bool    fitKc = +            "if Kc should be fit"
bool    fitnt = -
floatt  yt = 1.585            "specify yt"
floatt  yi = -0.81            "specify yi"
floatt  nt  = 0
int     minsystemsize = 4 m  "specify a minimum systemsize. Bigger systems will be ignored"
floatt  sccr=3.0       s     "specify 'scaling width criterium'"
