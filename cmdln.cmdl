floatt  a4  = 0
floatt  C   = -0.11
bool    fita0 = +             "wether a0 should be fit"
bool    fita1 = -             "wether a1 should be fit"
bool    fita2 = +             "wether a2 should be fit"
bool    fita3 = -             "wether a3 should be fit"
bool    fita4 = +             "wether a4 should be fit"
bool    fitC  = +             "wether C should be fit"
bool    formC = -             "wether C in formula, this can be switched by sending C" 

