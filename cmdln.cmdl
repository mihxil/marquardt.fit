floatt  conv=0.2e-7          "halting criteria"
bool    anisotropic = +      "wether the variables for anistropic or isotropic model should be used \n this can be switched by sending C"
                            


*c_f2 g

floatt  conv=0.2e-7          "halting criteria"
floatt  a0 = 0.6236          "starting value for a0"
